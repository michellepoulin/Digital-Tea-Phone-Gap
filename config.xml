<?xml version-"1.0" encoding="UTF-8"?>
<!-- config.xml reference: https://build.phonegap.com/docs/config-xml -->

<widget xmlns = "http://www.w3.org/ns/widgets"
		xmlns:gap = "http://phonegap.com/ns/1.0"
		xmlns:android = "http://schemas.android.com/apk/res/android"
		id = "com.phonegap.example"
		version = "1.1.0"/>

	<name>Employee Directory</name>
	<description>A Simple Employee Directory</description>
	<author href="http://digitaltea.com" email="michelle@digitaltea.com">Michelle Poulin</author>
	<feature name="http://api.phonegap.com/1.0/camera"/>
	<feature name="http://api.phonegap.com/1.0/contacts"/>
	<feature name="http://api.phonegap.com/1.0/file"/>
	<feature name="http://api.phonegap.com/1.0/geolocation"/>
	<feature name="http://api.phonegap.com/1.0/media"/>
	<feature name="http://api.phonegap.com/1.0/network"/>
	<feature name="http://api.phonegap.com/1.0/notification"/>
	<gap:plugin name="org.apache.cordova.dialogs" />
	<feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
	<preference name="permissions" value="none"/>
	<preference name="phonegap-version" value="2.0" />       
</widget>		